{ "manifest_version": 2
, "name": "Book walker"
, "description": "Esunbank helper"
, "version": "1.0"

, "permissions":
  [ "https://*.bookwalker.com.tw/*"
  , "webRequest"
  , "background"
  ]

, "background":
  { "scripts": ["background.js"]
  }

, "browser_action":
  { "default_popup": "popup.html"
  }
}
